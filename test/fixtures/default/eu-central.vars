stack   = {
  name = "test_starter_stack"
  network_cidr = "10.1.0.0/16"
  zone = "eu-central"
  ssh_key_name = "default-key"
  domain = "prvsn.dev"

  servers = [
    {
      name = "web-1"
      type = "cpx11"
      data_volume_size = 30
